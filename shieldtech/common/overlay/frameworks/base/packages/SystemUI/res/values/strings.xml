<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Content description of the RS Mouse tile in quick settings (not shown on the screen). [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="accessibility_quick_settings_rsmouse" />
    <string name="accessibility_quick_settings_rsmouse">Virtual Mouse <xliff:g id="state" example="Off">%s</xliff:g>.</string>

    <!-- QuickSettings: RSMouse On [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="quick_settings_rsmouse_on_label" />
    <string name="quick_settings_rsmouse_on_label">Virtual Mouse On</string>
    <!-- QuickSettings: RSMouse Off [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="quick_settings_rsmouse_off_label" />
    <string name="quick_settings_rsmouse_off_label">Virtual Mouse Off</string>
    <!-- QuickSettings: RSMouse Default Value -->
    <add-resource type="string" name="quick_settings_rsmouse_default" />
    <string name="quick_settings_rsmouse_default">1</string>

    <!-- Content description of the full screen mode tile in quick settings (not shown on the screen). [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="accessibility_quick_settings_fullscreen" />
    <string name="accessibility_quick_settings_fullscreen">Full screen mode <xliff:g id="state" example="Off">%s</xliff:g>.</string>
    <!-- QuickSettings: Full screen On [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="quick_settings_fullscreen_on_label" />
    <string name="quick_settings_fullscreen_on_label">Fullscreen On</string>
    <!-- QuickSettings: Full screen Off [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="quick_settings_fullscreen_off_label" />
    <string name="quick_settings_fullscreen_off_label">Fullscreen Mode</string>
    <!-- QuickSettings: Full screen Default Value -->
    <add-resource type="string" name="quick_settings_fullscreen_default" />
    <string name="quick_settings_fullscreen_default">0</string>
</resources>

