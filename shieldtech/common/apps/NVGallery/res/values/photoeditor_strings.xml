    <string name="crop_tooltip">Drag markers to crop photo</string>

    <!-- Tool-tip toast shown when the user chooses to doodle on photo. [CHAR LIMIT=40] -->
    <string name="doodle_tooltip">Draw on photo to doodle</string>

